{
  "name": "starterkit",
  "type": "module",
  "author": "Cyril Cornez",
  "scripts": {
    "start": "parcel serve ./src/index.html --open",
    "clean": "rm -rf ./dist ./.parcel-cache ./out",
    "build": "npm run clean && parcel build ./src/index.html --dist-dir ./out --no-source-maps && npm run serve",
    "serve": "live-server ./out",
    "develop": "surge ./out --domain https://cc-develop.surge.sh",
    "deploy": "surge ./out --domain https://cc-portfolio.surge.sh"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@parcel/transformer-sass": "^2.7.0",
    "posthtml-include": "^1.7.1"
  }
}
